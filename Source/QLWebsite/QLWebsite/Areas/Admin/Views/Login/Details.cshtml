@model QLWebsite.Models.TaiKhoan

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Manager/Views/Shared/_ManagerDefault.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3>Chi tiết tài khoản</h3>
            </div>
            <div>
                <dl class="dl-horizontal">
                    <dt>
                        Tên tài khoản
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.TenTaiKhoan)
                    </dd>

                    <dt>
                        Mật khẩu
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.MatKhau)
                    </dd>

                    <dt>
                        Trạng thái
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.TrangThai)
                    </dd>

                    <dt>
                        Quyền
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Quyen)
                    </dd>

                    <dt>
                        Người dùng
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.MaND)
                    </dd>

                </dl>
            </div>
            <p>
                @Html.ActionLink("Sửa", "Edit", new { id = Model.ID }) |
                @Html.ActionLink("Quay lại", "Index")
            </p>
            </div>
        </div>
    </div>
