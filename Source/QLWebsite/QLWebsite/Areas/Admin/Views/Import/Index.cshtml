@{
    ViewBag.Title = "Import";
    Layout = "~/Areas/Admin/Views/Shared/_AdminDefault.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div><p align="center" style="font-size:23px; margin-top:6px; border:solid 1px black">Nhập dữ liệu vào cở sở dữ liệu</p></div>
            <div class="form-horizontal">
                @using (Html.BeginForm("Importexcel", "Import", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            <div class="form-group">
                                <label class="control-label col-md-2">Excel file</label>
                                <div class="col-md-10">
                                    <input type="file" id="FileUpload1" name="FileUpload1" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-2"></label>
                                <div class="col-md-10">
                                    <input type="submit" name="Submit" value="Import" />
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>

